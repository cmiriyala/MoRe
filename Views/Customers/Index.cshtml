@model MoRe.ViewModels.RandomMovieViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customers</h2>


<table class="table">
    <tr>
        <th>
            Customers
        </th>
        <th></th>
    </tr>

    @foreach (var customer in Model.Customers)
    {
        <tr>
            <td>
                @Html.ActionLink(customer.Name, "View", "Customers", new { id = customer.Id, name = customer.Name }, null)

            </td>
        </tr>

    }

</table>
