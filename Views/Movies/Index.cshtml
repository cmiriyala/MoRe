@model MoRe.ViewModels.RandomMovieViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>


<table class="table">
    <tr>
        <th>
            Movies
        </th>
        <th></th>
    </tr>

    @foreach (var movie in Model.Movies)
    {
        <tr>
            <td> @Html.ActionLink(movie.Name, "View", "Movies", new { id = movie.Id, name = movie.Name }, null)</td> 
        </tr>

            }
      
    </table>
